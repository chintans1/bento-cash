{
  "expo": {
    "name": "Bento Cash",
    "slug": "LM-Mobile",
    "version": "0.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "buildNumber": "13",
      "bundleIdentifier": "com.chintans98.lunchmoney.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ee05cd6b-800e-4794-aa79-c953528fcdf3"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
